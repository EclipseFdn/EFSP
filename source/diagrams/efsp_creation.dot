 digraph {
	bgcolor=transparent;
	rankdir=TB;
	splines=ortho;
	
	start[shape="circle";label=""];
	proposal[shape="box"; label="Project Team\nPresents Proposal\nto the EMO"];
	ed_approval[shape="box"; label="EMO(ED)\nApproval"];
	start_community_review[shape="box"; label="Start\nCommunity\nReview"];
	{
		{
			rank=same;rankdir="TB";
			is_patent_license[shape="diamond";label="Does Patent License\nmatch the default?";fontsize="10"];
			is_patent_license_yes[style="invis",fixedsize=true,width=0,height=0];
		}
		{
			rank=same;rankdir="TB";
			steering_committee_vote[shape="box";label="Steering Committee\nVotes"];
			steering_committee_approval[style="invis",fixedsize=true,width=0,height=0];
		}
	}
	
	{
		review[shape="box";label="Start\nCreation\nReview"];
		
		{
			rank=same;rankdir=LR;
			pmc_approval[shape="box";label="PMC Approval"];
			community_review[shape="none";label="ðŸ“†\nMinimum\nOne Week"];
			specification_committee_ballot[shape="box";label="Specification Committee\nBallots"];
		}
		
		success[shape="box";label="End\nCreation\nReview"];
	}
	
	provision_project[shape="box";label="Initiate\nProject Provisioning"];
	
	start -> proposal;
	proposal -> ed_approval;
	ed_approval-> start_community_review[label=" approve"];
	start_community_review -> is_patent_license;
	is_patent_license -> steering_committee_vote [label=" no"];
	is_patent_license -> is_patent_license_yes[arrowhead="none"];
	is_patent_license_yes -> review [label=" yes"];
	steering_committee_vote -> steering_committee_approval[arrowhead="none"];
	steering_committee_approval -> review [xlabel="approve "];
	review -> pmc_approval;
	pmc_approval -> success[xlabel="+1 "];
	review -> specification_committee_ballot;
	specification_committee_ballot -> success[label="approve"];
	review -> community_review -> success;
	success -> provision_project;
	
}